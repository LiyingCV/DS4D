ninja_required_version = 1.3
cxx = c++
nvcc = /public/nongjian/lyyang/tools/cuda-11.8/bin/nvcc

cflags = -pthread -B /public/nongjian/lyyang/anaconda3/envs/stag4d/compiler_compat -Wno-unused-result -Wsign-compare -DNDEBUG -fwrapv -O2 -Wall -fPIC -O2 -isystem /public/nongjian/lyyang/anaconda3/envs/stag4d/include -fPIC -O2 -isystem /public/nongjian/lyyang/anaconda3/envs/stag4d/include -fPIC -I/public/nongjian/lyyang/anaconda3/envs/stag4d/lib/python3.10/site-packages/torch/include -I/public/nongjian/lyyang/anaconda3/envs/stag4d/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -I/public/nongjian/lyyang/anaconda3/envs/stag4d/lib/python3.10/site-packages/torch/include/TH -I/public/nongjian/lyyang/anaconda3/envs/stag4d/lib/python3.10/site-packages/torch/include/THC -I/public/nongjian/lyyang/tools/cuda-11.8/include -I/public/nongjian/lyyang/anaconda3/envs/stag4d/include/python3.10 -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=_C -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++17
cuda_cflags = -I/public/nongjian/lyyang/anaconda3/envs/stag4d/lib/python3.10/site-packages/torch/include -I/public/nongjian/lyyang/anaconda3/envs/stag4d/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -I/public/nongjian/lyyang/anaconda3/envs/stag4d/lib/python3.10/site-packages/torch/include/TH -I/public/nongjian/lyyang/anaconda3/envs/stag4d/lib/python3.10/site-packages/torch/include/THC -I/public/nongjian/lyyang/tools/cuda-11.8/include -I/public/nongjian/lyyang/anaconda3/envs/stag4d/include/python3.10 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -I/public/nongjian/lyyang/works/4d/STAG4D/diff-gaussian-rasterization/third_party/glm/ -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=_C -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_86,code=compute_86 -gencode=arch=compute_86,code=sm_86 -std=c++17
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags





build /public/nongjian/lyyang/works/4d/STAG4D/diff-gaussian-rasterization/build/temp.linux-x86_64-cpython-310/cuda_rasterizer/backward.o: cuda_compile /public/nongjian/lyyang/works/4d/STAG4D/diff-gaussian-rasterization/cuda_rasterizer/backward.cu
